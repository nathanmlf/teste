programa {
  funcao inicio() {

    inteiro idade
    escreva("Digite a sua idade: ")
    leia(idade)

    se (idade < 18)
    {
      escreva("Você é de menor!")
    }
    senao 
    {
      escreva("Você é de maior!")
    }
  }
}

//Peça a idade do usuário.
//Se for 18 ou mais, exiba "Você é maior de idade".
//Caso contrário, exiba "Você é menor de idade".