programa 
{
    funcao inicio() 
    {
      inteiro num1, num2

      escreva("Digite um número inteiro: ")
      leia(num1)

      escreva("Digite outro número inteiro: ")
      leia(num2)

      se (num1 < num2)
      {
        escreva("O numero maior é : ", num2)
      } 
      senao 
      {
        escreva("O numero maior é : ", num1)
      }
    }
}

